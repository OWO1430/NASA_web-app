generator client {
  provider = "prisma-client-js"
  output   = "./generated/client"
}

datasource db {
  provider = "sqlite"
  // url      = "file:./data.sqlite3"
  url = env("POSTGRES_PRISMA_URL") // uses connection pooling
  directUrl = env("POSTGRES_URL_NON_POOLING") // uses a direct connection
}

model Asteroids {
  full_name String @id
  a         Float
  e         Float
  I         Float
  L         Float
  longPeri  Float
  longNode  Float
  size      Float
  H         Float
  pha       String
}

model Planets {
  full_name String @id
  a         Float
  e         Float
  I         Float
  L         Float
  longPeri  Float
  longNode  Float
  size      Float
  axialTilt Float
  rotPeriod Float
  tex       String
}
